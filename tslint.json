{
	"rules": {
		"adjacent-overload-signatures": true,
		"align": [
			true,
			"parameters",
			"statements"
		],
		"array-type": [
			true,
			"array-simple"
		],
		"arrow-parens": true,
		"class-name": true,
		"comment-format": [
			true,
			"check-space"
		],
		"curly": true,
		"cyclomatic-complexity": [
			false
		],
		"eofline": false,
		"forin": true,
		"indent": [
			true,
			"tabs"
		],
		"interface-name": [
			true,
			"always-prefix"
		],
		"jsdoc-format": false,
		"label-position": true,
		"max-classes-per-file": [
			true,
			1
		],
		"max-line-length": [
			false
		],
		"member-access": true,
		"member-ordering": [
			true,
			{
				"order": "statics-first"
			}
		],
		"new-parens": true,
		"no-any": true,
		"no-arg": true,
		"no-bitwise": true,
		"no-conditional-assignment": true,
		"no-consecutive-blank-lines": [
			true,
			2
		],
		"no-console": [
			true,
			"debug",
			"info",
			"log",
			"time",
			"timeEnd",
			"trace"
		],
		"no-construct": true,
		"no-debugger": true,
		"no-empty": true,
		"no-eval": true,
		"no-internal-module": true,
		"no-namespace": true,
		"no-parameter-properties": false,
		"no-reference": true,
		"no-shadowed-variable": true,
		"no-string-literal": true,
		"no-switch-case-fall-through": false,
		"no-trailing-whitespace": true,
		"no-unsafe-finally": true,
		"no-unused-expression": true,
		"no-unused-new": true,
		"no-use-before-declare": false,
		"no-var-keyword": true,
		"no-var-requires": true,
		"object-literal-key-quotes": [
			true,
			"as-needed"
		],
		"object-literal-shorthand": true,
		"object-literal-sort-keys": false,
		"one-line": [
			false
		],
		"one-variable-per-declaration": [
			false
		],
		"only-arrow-functions": [
			true,
			"allow-declarations"
		],
		"ordered-imports": [
			true,
			{
				"import-sources-order": "case-insensitive",
				"named-imports-order": "case-insensitive"
			}
		],
		"prefer-for-of": true,
		"quotemark": [
			true,
			"single",
			"avoid-escape"
		],
		"radix": true,
		"semicolon": [
			true,
			"always"
		],
		"switch-default": true,
		"trailing-comma": [
			false
		],
		"triple-equals": [
			true,
			"allow-null-check"
		],
		"typedef": [
			true,
			"call-signature",
			"property-declaration",
			"member-variable-declaration",
			"parameter"
		],
		"typedef-whitespace": [
			true,
			{
				"call-signature": "nospace",
				"index-signature": "nospace",
				"parameter": "nospace",
				"property-declaration": "nospace",
				"variable-declaration": "nospace"
			},
			{
				"call-signature": "onespace",
				"index-signature": "onespace",
				"parameter": "onespace",
				"property-declaration": "onespace",
				"variable-declaration": "onespace"
			}
		],
		"use-isnan": true,
		"variable-name": [
			true,
			"allow-leading-underscore",
			"ban-keywords",
			"check-format",
			"allow-pascal-case"
		],
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-type"
		]
	}
}